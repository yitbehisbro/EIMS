{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "EdrIMSConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=DB_NAME;Integrated Security=True;TrustServerCertificate=True;MultipleActiveResultSets=True;"
  },

  "Keycloak": {
    "ServerRealm": "http://localhost:8080/realms/E-IMS",
    "Metadata": "http://localhost:8080/realms/E-IMS/.well-known/openid-configuration",
    "ClientId": "CLIENT_ID",
    "ClientSecret": "CLIENT_SECRET_CODE",
    "TokenExchange": "http://localhost:8080/realms/E-IMS/protocol/openid-connect/token",
    "Audience": "some-audience",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath": "/signout-callback-oidc"
  },
  "AllowedHosts": "*"
}
